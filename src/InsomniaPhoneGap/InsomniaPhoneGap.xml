<?xml version="1.0" encoding="utf-8" ?>
<widget id="InsomniaPhoneGap.widget.InsomniaPhoneGap" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Insomnia Widget</name>
    <description>Prevent iOS and Android devices from falling asleep</description>
    <icon>
        
    </icon>
    <phonegap enabled="true"/>
    <properties>   
        <property key="keepAwake" type="attribute" required="true">
            <caption>Keep Awake</caption>
            <category>Behavior</category>
            <description>When enabled, the plugin will keep the device awake.
            </description> 
            <attributeTypes>
                <attributeType name="Boolean"/>
            </attributeTypes>
        </property>
        <property key="onchangemf" type="microflow" required="false">
            <caption>On change microflow</caption>
            <category>Events</category>
            <description>Microflow executed when the toggle is clicked.</description> 
            <returnType type="Void"/>
        </property>
        <property key="onChangeNanoflow" type="nanoflow" required="false">
            <caption>On change nanoflow</caption>
            <category>Events</category>
            <description>The nanoflow to call when the toggle is clicked.</description>
            <returnType type="Void"/>
        </property>
    </properties>
</widget>
